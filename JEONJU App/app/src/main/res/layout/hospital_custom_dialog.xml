<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        android:id="@+id/hospitalDialogTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="21sp"
        android:textColor="#3f9bbf"
        android:textStyle="bold"
        android:text="병원/약국"
        android:layout_marginTop="15dp"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="21dp"
        android:layout_marginBottom="8dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textColor="#000000"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="21dp"
        android:layout_marginBottom="6dp"
        android:textStyle="bold"
        android:text="주소" />
    <TextView
        android:id="@+id/hospitalAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textColor="#000000"
        android:layout_marginBottom="9dp"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="31dp"
        android:text="주소 내용" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textColor="#000000"
        android:layout_marginBottom="6dp"
        android:textStyle="bold"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="21dp"
        android:text="분류" />
    <TextView
        android:id="@+id/hospitalSort"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textColor="#000000"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="31dp"
        android:layout_marginBottom="9dp"
        android:text="분류 내용" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textColor="#000000"
        android:layout_marginBottom="6dp"
        android:textStyle="bold"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="21dp"
        android:text="운영 시간" />
    <TextView
        android:id="@+id/hospitalTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textColor="#000000"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="31dp"
        android:text="운영 시간 내용" />

    <LinearLayout
        android:id="@+id/hospitalDay"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="21dp"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/hospitalDayMon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="월" />
        <TextView
            android:id="@+id/hospitalDayTue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="화" />
        <TextView
            android:id="@+id/hospitalDayWed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="수" />
        <TextView
            android:id="@+id/hospitalDayThu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="목" />
        <TextView
            android:id="@+id/hospitalDayFri"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="금" />
        <TextView
            android:id="@+id/hospitalDaySat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="토" />
        <TextView
            android:id="@+id/hospitalDaySun"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="일" />
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textColor="#000000"
        android:layout_marginBottom="6dp"
        android:textStyle="bold"
        android:layout_marginLeft="21dp"
        android:layout_marginRight="21dp"
        android:text="전화 번호" />
    <TextView
        android:id="@+id/hospitalTel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:textColor="#000000"
        android:layout_marginBottom="9dp"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="31dp"
        android:text="전화 번호 내용" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textColor="#000000"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="21dp"
        android:textStyle="bold"
        android:text="응급실 운영" />
    <LinearLayout
        android:layout_marginRight="21dp"
        android:layout_marginLeft="21dp"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/hospitalEmergencyYes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="예" />
        <TextView
            android:id="@+id/hospitalEmergencyNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="아니오" />
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textColor="#000000"
        android:layout_marginRight="21dp"
        android:layout_marginLeft="21dp"
        android:textStyle="bold"
        android:text="공휴일 진료" />
    <LinearLayout
        android:layout_marginRight="21dp"
        android:layout_marginLeft="21dp"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/hospitalHolidayYes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="예" />
        <TextView
            android:id="@+id/hospitalHolidayNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="#000000"
            android:padding="9dp"
            android:text="아니오" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/hospitalDialogCancel"
            android:onClick="onClick"
            android:text="취소"
            android:textColor="#333333"
            android:textSize="18sp"
            android:layout_marginLeft="21dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="17dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"/>
        <TextView
            android:id="@+id/hospitalDialogCall"
            android:text="전화"
            android:textSize="18sp"
            android:layout_marginRight="30dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="17dp"
            android:textColor="#0d2457"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/hospitalDialogMap"
            android:text="길안내"
            android:textColor="#0d2457"
            android:textSize="18sp"
            android:layout_marginRight="21dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="17dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

</LinearLayout>